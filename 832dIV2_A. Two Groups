//https://codeforces.com/contest/1747/problem/A

#include<bits/stdc++.h>
#define ll long long
#define lld long double
#define fl(i,n) for(int i=0;i<n;i++)
#define rl(i,m,n) for(int i=n;i>=m;i--)
#define pb push_back
using namespace std;
 
int main()
{
    ll n;
    cin>>n;
    while(n--){
        ll m;
        cin>>m;
        ll sum=0;
        while(m--){
            ll p;
            cin>>p;
            sum+=p;
        }
        cout<<abs(sum)<<"\n";
    }
 
    return 0;
}
